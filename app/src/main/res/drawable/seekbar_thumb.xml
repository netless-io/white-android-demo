<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid android:color="@color/color_slect_7"/>
            <size
                android:width="12dp"
                android:height="12dp"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid android:color="@color/color_slect_7"/>
            <size
                android:width="12dp"
                android:height="12dp"/>
        </shape>
    </item>
    <item>
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid android:color="@color/white_default"/>
            <size
                android:width="12dp"
                android:height="12dp"/>
        </shape>
    </item>
</selector>
